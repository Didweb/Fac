 <div id="crear_telefono">
 <form action="{{ path('ofi_gestion_crearctelefono',{'id': entity.empresa.id}) }}" method="post" {{ form_enctype(form_telefono) }}>
        <input type="hidden" name="_method" value="PUT" />
        {{ form_errors(form_telefono) }}

			<div id="c_telefono_nombre">
				{{ form_label(form_telefono.nombre,"Nombre:") }}
				{{ form_errors(form_telefono.nombre) }}
				{{ form_widget(form_telefono.nombre) }}
			</div>
			
			<div id="c_telefono_telefono">
				{{ form_label(form_telefono.telefono,"Teléfono:") }}
				{{ form_errors(form_telefono.telefono) }}
				{{ form_widget(form_telefono.telefono) }}
			</div>
			
			<div id="c_telefono_tipo">
				{{ form_label(form_telefono.tipo,"Cargo:") }}
				{{ form_errors(form_telefono.tipo) }}
				{{ form_widget(form_telefono.tipo) }}
			</div>		

			
			<div id="datos_envio">
				<button type="submit"  class="botones_verde" 
				title="Editar Teléfono" >Crear Teléfono</button>
		    </div>
		    
         {{ form_rest(form_telefono) }}
 </form> 
 </div>
